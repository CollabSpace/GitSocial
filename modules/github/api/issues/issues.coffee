Namespace 'GitHub.api',
  'GET /issues': 'getIssues'
  'GET /user/issues': 'getMyIssues'
  'GET /orgs/:org/issues': 'getOrgIssues'
  'GET /repos/:owner/:repo/issues': 'getRepoIssues'
  'GET /repos/:owner/:repo/issues/:number': 'getIssue'
  'POST /repos/:owner/:repo/issues': 'createIssue'
  'PATCH /repos/:owner/:repo/issues/:number': 'editIssue'
